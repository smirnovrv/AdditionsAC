Процедура УстановитьОтборРегистраСтавок()
	
	Запрос = Новый Запрос;
	Запрос.Текст =
	"ВЫБРАТЬ
	|	ИИТ_ИсторияИзмененияСтавок.Период КАК Период,
	|	ИИТ_ИсторияИзмененияСтавок.Ставка КАК Ставка
	|ИЗ
	|	РегистрСведений.ИИТ_ИсторияИзмененияСтавок КАК ИИТ_ИсторияИзмененияСтавок
	|ГДЕ
	|	ИИТ_ИсторияИзмененияСтавок.ФизЛицо = &ФизЛицо
	|
	|УПОРЯДОЧИТЬ ПО
	|	Период";
	Запрос.УстановитьПараметр("ФизЛицо", ФизическоеЛицо.Ссылка);
	
	ИИТ_ИсторияСтавок.Загрузить(Запрос.Выполнить().Выгрузить());

КонецПроцедуры

//ИИТ_Аксеньюшкин
Процедура ЗаписатьИсториюСтавок()
	
	НаборЗаписей = РегистрыСведений.ИИТ_ИсторияИзмененияСтавок.СоздатьНаборЗаписей();
	НаборЗаписей.Отбор.ФизЛицо.Установить(ФизическоеЛицо.Ссылка);
	НаборЗаписей.Прочитать();
	НаборЗаписей.Очистить();
	НаборЗаписей.Записать(Истина);
	
	СправочникОбъект = ФизическоеЛицо.Ссылка.ПолучитьОбъект();

	Для Каждого СтрокаИстории Из ИИТ_ИсторияСтавок Цикл
		НоваяЗапись = РегистрыСведений.ИИТ_ИсторияИзмененияСтавок.СоздатьМенеджерЗаписи();
		НоваяЗапись.Период = СтрокаИстории.Период;
		НоваяЗапись.Ставка = СтрокаИстории.Ставка;
		НоваяЗапись.ФизЛицо = ФизическоеЛицо.Ссылка;
		НоваяЗапись.Записать(Истина);
		
		СправочникОбъект.ИИТ_ОкладВЧас = СтрокаИстории.Ставка;
	КонецЦикла;
	
	СправочникОбъект.Записать();
	
КонецПроцедуры

Процедура ЗаписатьИсториюОкладов()
	
	НаборЗаписей = РегистрыСведений.АС_ИсторияИзмененияОклада.СоздатьНаборЗаписей();
	НаборЗаписей.Отбор.ФизЛицо.Установить(ФизическоеЛицо.Ссылка);
	НаборЗаписей.Прочитать();
	НаборЗаписей.Очистить();
	НаборЗаписей.Записать(Истина);
	
	Для Каждого СтрокаИстории Из АС_ИсторияОкладов Цикл
		НоваяЗапись = РегистрыСведений.АС_ИсторияИзмененияОклада.СоздатьМенеджерЗаписи();
		НоваяЗапись.Период 		= СтрокаИстории.Период;
		НоваяЗапись.Оклад 		= СтрокаИстории.Оклад;
		НоваяЗапись.ТипОклада 	= СтрокаИстории.ТипОклада;
		НоваяЗапись.ФизЛицо 	= ФизическоеЛицо.Ссылка;
		НоваяЗапись.Записать(Истина);
	КонецЦикла;

КонецПроцедуры

Процедура УстановитьОтборРегистраОкладов()
	
	Запрос = Новый Запрос;
	Запрос.Текст =
	"ВЫБРАТЬ
	|	АС_ИсторияИзмененияОклада.Период КАК Период,
	|	АС_ИсторияИзмененияОклада.ФизЛицо КАК ФизЛицо,
